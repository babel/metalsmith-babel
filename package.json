{
  "name": "metalsmith-babel",
  "version": "2.0.0",
  "description": "babel plugin for Metalsmith",
  "repository": "babel/metalsmith-babel",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc-node/rc.json --rule 'no-underscore-dangle: 0' index.js test.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/babel/metalsmith-babel/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "metalsmith",
    "metalsmith-plugin",
    "babel",
    "ecmascript",
    "script",
    "transpile",
    "transpiler",
    "language",
    "es6",
    "plugin",
    "harmony"
  ],
  "dependencies": {
    "babel-core": "^5.0.0",
    "object-assign": "^2.0.0"
  },
  "devDependencies": {
    "@shinnn/eslintrc-node": "^1.0.2",
    "eslint": "^0.20.0",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "metalsmith": "^1.6.0",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  }
}
